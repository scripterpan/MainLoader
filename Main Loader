-- Define supported Place IDs
local supportedGames = {
    [6243699076] = "The Mimic",  -- The Mimic
    [15532962292] = "Sol's RNG"  -- Sol's RNG
}

-- Get services
local StarterGui = game:GetService("StarterGui")
local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- Function to send notifications
local function sendNotification(title, text, duration)
    StarterGui:SetCore("SendNotification", {
        Title = title,
        Text = text,
        Duration = duration or 5 -- Default duration is 5 seconds
    })
end

-- Function to execute the Mimic script
local function executeMimicScript()
    -- Insert the Mimic specific logic here
    loadstring(game:HttpGet("https://raw.githubusercontent.com/scripterpan/scripterpan/refs/heads/main/TheMimic"))()
    sendNotification("Script Loaded", "The Mimic script has successfully loaded. Enjoy!", 5)
    -- Additional code for the Mimic can go here
end

-- Notify that the script is starting
sendNotification("Script Initialized", "Starting game compatibility check...", 5)
task.wait(2)

-- Notify that the script is checking the Place ID
sendNotification("Checking Place ID", "Verifying the game's compatibility with the script...", 5)
task.wait(3)

-- Check if the current game is supported
local gameName = supportedGames[game.PlaceId]
if gameName then
    -- Notify that the game is supported
    sendNotification("Game Supported", "Welcome to " .. gameName .. "! The script is loading...", 5)
    task.wait(3)

    -- Provide additional confirmation
    sendNotification("Loading Script", "Please wait while the script initializes for " .. gameName .. ".", 5)
    task.wait(2)

    -- If it's The Mimic, show the recommendation
    if game.PlaceId == 6243699076 then
        sendNotification("Execution Recommendation", "We recommend executing the script in the lobby instead of in-game due to bugs right now.", 7)
        task.wait(3)

        -- Check if it's the first time entering the game
        local firstExecution = player:HasEnteredPlace(6243699076) == false
        if firstExecution then
            -- Queue the function for teleportation (auto-execute)
            sendNotification("Auto Execution", "Executing script for The Mimic automatically...", 5)
            -- Trigger the script execution function after teleport
            TeleportService:Teleport(6243699076, player)  -- Teleport to the same game and auto-execute
            task.wait(1)
            executeMimicScript()
        else
            -- If not the first execution, just execute the script
            executeMimicScript()
        end
    -- Execute Sol's RNG script
    elseif game.PlaceId == 15532962292 then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/scripterpan/scripterpan/refs/heads/main/SolsRNG"))()
        sendNotification("Script Loaded", "Sol's RNG script has successfully loaded. Have fun!", 5)
        -- Insert Sol's RNG script logic here
    end
else
    -- Notify that the game is unsupported
    sendNotification("Unsupported Game", "This game is unsupported, and we'll make a universal script soon!", 5)
    task.wait(3)

    -- Additional guidance for the user
    sendNotification("What to Do Next?", "Try using this script in a supported game like The Mimic or Sol's RNG etc.", 5)
end
